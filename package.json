{
    "name": "vue-auto-create-route",
    "version": "1.0.12",
    "description": "hbq-vue-auto-create-route",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "main": "build/index.js",
    "scripts": {
        "start:build": "npm run tsc && node build",
        "start": "node run",
        "test": "jest --forceExit --detectOpenHandles",
        "clear": "rimraf build",
        "tsc": "npm run clear && tsc",
        "tsc:w": "npm run tsc -- -w",
        "p": "npm run tsc && npm publish",
        "lint": "tslint \"src/**/*.ts\" --fix",
        "precommit": "npm run lint",
        "examples": "ts-node './src/examples'"
    },
    "dependencies": {
        "chokidar": "^3.4.2",
        "fs-extra": "^9.0.1",
        "globby": "^11.0.1",
        "js-beautify": "^1.13.0",
        "lodash.debounce": "^4.0.8",
        "naming-style": "^1.0.1",
        "tslib": "^1.13.0"
    },
    "devDependencies": {
        "@types/node": "^12.12.53",
        "husky": "^4.2.3",
        "jest": "^25.5.4",
        "rimraf": "^2.7.1",
        "ts-node": "^8.10.2",
        "tslint": "^6.1.2",
        "typescript": "^3.9.7",
        "webpack": "^4.44.2"
    },
    "keywords": [
        "typescript",
        "module-polierplate",
        "hbq-vue-auto-create-route",
        "webpackplugin"
    ],
    "husky": {
        "hooks": {"pre-commit": "npm run lint"}
    },
    "author": "hubiqing",
    "repository": "https://github.com/githbq/hbq-vue-auto-create-route.git",
    "engines": {"node": ">=10"},
    "license": "MIT"
